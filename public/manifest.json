{
	"name": "template-extensions",
	"short_name": "template-extensions",
	"version": "0.0.1",
	"manifest_version": 3,
	"permissions": [],
	"host_permissions": [
		"*://*/*"
	],
	"content_scripts": [
		{
			"matches": [
				"http://*/*",
				"https://*/*"
			],
			"js": [
				"content_script.js"
			]
		}
	],
	"web_accessible_resources": [
		{
			"resources": [
				"injects/*"
			],
			"matches": [
				"<all_urls>"
			]
		}
	]
}